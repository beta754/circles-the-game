{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2019",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2019",
      "ES2019.Array"
    ],
    "resolveJsonModule": true,
    "outDir": "dist",
    "experimentalDecorators": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}